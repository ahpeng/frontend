{
  "id": "frontend",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "flakio/frontend:latest",
      "privileged": false,
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "parameters": []
    }
  },
  "instances": 2,
  "cpus": 0.1,
  "mem": 128,
  "labels": {
    "HAPROXY_GROUP":"external",
    "HAPROXY_0_VHOST":"{{dns_label}}.{{location}}.cloudapp.azure.com",
    "HAPROXY_0_PATH":"/"
  }
}
